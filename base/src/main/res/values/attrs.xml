<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OctoTextInputLayout">
        <attr name="label" format="string" />
        <attr name="defaultInputValue" format="string" />
        <attr name="actionDrawable" format="reference" />
    </declare-styleable>
    <declare-styleable name="TemperatureView">
        <attr name="maxTemp" format="integer" />
    </declare-styleable>
    <declare-styleable name="OctoView">
        <attr name="octoActivity" format="enum">
            <enum name="idle" value="0" />
            <enum name="swim" value="1" />
        </attr>
    </declare-styleable>
</resources>