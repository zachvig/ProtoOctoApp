<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OctoTextInputLayout">
        <attr name="icon" format="reference" />
        <attr name="iconTint" format="color" />
        <attr name="label" format="string" />
        <attr name="selectAllOnFocus" format="boolean" />
        <attr name="labelActive" format="string" />
        <attr name="example" format="string" />
        <attr name="defaultInputValue" format="string" />
        <attr name="actionDrawable" format="reference" />
        <attr name="actionOnlyWithText" format="boolean" />
        <attr name="forPassword" format="boolean" />
        <attr name="android:autofillHints" format="string" />
    </declare-styleable>
    <declare-styleable name="OctoView">
        <attr name="octoActivity" format="enum">
            <enum name="idle" value="0" />
            <enum name="swim" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="TutorialView">
        <attr name="title" format="string" />
        <attr name="detail" format="string" />
        <attr name="hidePrefName" format="string" />
        <attr name="hidePrefInverted" format="boolean" />
        <attr name="hideButton" format="string" />
        <attr name="animations" format="boolean" />
        <attr name="hero" format="reference" />
        <attr name="learnMoreButton" format="string" />
        <attr name="learnMoreIcon" format="reference" />
        <attr name="noBottomMargin" format="boolean" />
        <attr name="alwaysShow" format="boolean" />
    </declare-styleable>
</resources>